stock_price = {
    "AAPL": 180,
    "TSLA": 250,
    "GOOG": 140
}

total = 0

n = int(input("Enter how many stocks you want: "))

for i in range(n):
    stock = input("Enter stock name: ")
    quantity = int(input("Enter quantity: "))
    
    stock = stock.upper()
    
    if stock in stock_price:
        price = stock_price[stock]
        value = price * quantity
        print("Value of", stock, "is", value)
        total = total + value
    else:
        print("Stock not found")

print("Total investment is", total)

        print("Amount for", name, "=", amount)
    else:
        print("Stock not available")

print("Total Investment =", total)
